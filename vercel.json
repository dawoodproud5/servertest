{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/user/(.*)",
      "dest": "/api/user/$1"
    },
    {
      "src": "/api/candidate/(.*)",
      "dest": "/api/candidate/$1"
    },
    {
      "src": "/api/client/(.*)",
      "dest": "/api/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
